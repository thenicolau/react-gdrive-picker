{
  "name": "react-gdrive-picker",
  "version": "0.5.0",
  "description": "A customizable, inline Google Drive file picker component for React. Browse folders, search files, preview thumbnails, and select single or multiple files.",
  "keywords": [
    "react",
    "google-drive",
    "file-picker",
    "drive-picker",
    "google-api",
    "component",
    "widget",
    "google",
    "drive",
    "picker"
  ],
  "author": "thenicolau",
  "repository": {
    "type": "git",
    "url": "https://github.com/thenicolau/react-gdrive-picker.git"
  },
  "bugs": {
    "url": "https://github.com/thenicolau/react-gdrive-picker/issues"
  },
  "license": "MIT",
  "sideEffects": [
    "*.css"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build && cp src/styles/drive-picker.css dist/styles.css",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4"
  }
}
